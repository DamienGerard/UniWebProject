{

    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://api.example.com/ownerhouses.json#",
    "type": "array",
	"minItems": 1, 
    "uniqueitems": true,
    "items":{
        "type":"object",
        "properties":{
            "f_name":{
                "type":"string",
                "minLength":1,
                "maxLength":50
            },
            "l_name":{
                "type":"string",
                "minLength":1,
                "maxLength":50
            },
            "profile_pic":{
                "type":"string",
                "minLength":1,
                "maxLength":50
            },
            "date_since":{
                "format":"date"
            },
            "user_name":{
                "type":"string",
                "minLength":1,
                "maxLength":255
            }
        },
        "required":["f_name","l_name","profile_pic","date_since","user_name"],
        "additionalProperties":false
    },
    "additionalProperties":false

}